{
    "name": "yb/umsdk",
    "authors": [
        {
            "name": "yaobiao",
            "email": "1315508912@qq.com"
        }
    ],
    "license": "MIT",
    "require": {
        "php": ">7.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^7.5",
        "jeroen-g/laravel-packager": "^2.3"
    },
    "autoload": {
        "psr-4": {
            "UMSdk\\": "src\\"
        },
        "classmap": [
            "src/"
        ]
    },
    "autoload-dev": {
        "classmap": [
            "tests/"
        ]
    },
    "extra": {
        "laravel": {
            "providers": [
                "UMSdk\\Service\\DumpServiceProvider"
            ]
        },
        "aliases": {
            "UMPushIos": "UMSdk\\Facade\\UMPushIos",
            "UMPushAndroid": "UMSdk\\Facade\\UMPushAndroid"
        }
    },
    "repositories": {
        "packagist": {
            "type": "composer",
            "url": "https://mirrors.aliyun.com/composer/"
        }
    }
}
